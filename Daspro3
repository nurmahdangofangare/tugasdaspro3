def lists_to_dict(keys, values):
    if len(keys) != len(values):
        print("Panjang kedua list harus sama.")
        return None
    
    result_dict = dict(zip(keys, values))
    return result_dict

keys = ['nama', 'umur', 'kota']
values = ['Johnny', 29, 'Chicago']

result = lists_to_dict(keys, values)

if result:
    print("Data Dictionary:")
    print(result)